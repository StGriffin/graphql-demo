

type Query{
    getAllUsers : [User]
    getUserById(id:ID!):User

}

type Mutation{
    createUser(userRequest:UserRequest) : User
    updateUser(userRequest:UserRequest) : User
    deleteUser(id:ID!) : Boolean

}

type User{
    id:ID
    creationTime:DateTime
    updateTime:DateTime
    userName:String
    mail:String
    role:Role
}
input UserRequest{
    id:ID
    userName:String
    mail:String
    role:Role
}


enum Role{
    ADMIN,USER
}

scalar DateTime